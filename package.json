{
  "license": "Apache-2.0",
  "dependencies": {
    "@collabs/collabs": "0.5.1",
    "@collabs/container": "0.5.1"
  },
  "devDependencies": {
    "@collabs/container-testing-server": "0.5.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "cross-env": "^7.0.3",
    "ts-node": "^10.1.0",
    "@types/node": "^15.6.1",
    "typescript": "^4.3.5",
    "ts-loader": "^9.2.5",
    "source-map-loader": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "react-dev-utils": "^12.0.0",
    "@types/react-dev-utils": "^9.0.8",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "@types/copy-webpack-plugin": "^8.0.1",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1"
  },
  "scripts": {
    "start": "container-testing-server dist/plaintext.html",
    "benchmarks": "node build/benchmarks/main.js",
    "dev": "npm run dev:site && npm run build:benchmarks",
    "dev:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack",
    "build": "npm run build:site && npm run build:benchmarks",
    "build:site": "cross-env TS_NODE_PROJECT='tsconfig-for-webpack-config.json' webpack --mode production --no-devtool",
    "build:benchmarks": "tsc -p tsconfig.benchmarks.json",
    "test": "npm-run-all test:*",
    "test:pkg": "pkg-ok",
    "test:format": "prettier --check .",
    "fix": "npm-run-all fix:*",
    "fix:format": "prettier --write .",
    "clean": "rimraf dist build"
  }
}
